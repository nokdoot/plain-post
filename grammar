LETTER          ->  '.'


                    #ascii 13
CR              ->  '\xd'

                    #ascii 10
LF              ->  '\xa'

#New Line
NL              ->  CR | LF | CR LF

LINE            ->  '.*'
                    NL | EOF

SPACE           ->  '\x20'

TAB             ->  '\x9'

#Blank Line
BL              ->  ( LETTER{0} | SPACE | TAB )+
                    NL

WHITESPACE      ->  ( SPACE+ | TAB+ | NL )


